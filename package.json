{
  "name": "code-oss-dev",
  "version": "1.96.0",
  "description": "Visual Studio Code - Open Source",
  "main": "./out/main.js",
  "scripts": {
    "setup": "node scripts/setup.js",
    "setup-ci": "npm ci && cd build && npm ci --ignore-scripts",
    "precommit": "node ./node_modules/gulp/bin/gulp.js electron",
    "compile": "node ./node_modules/gulp/bin/gulp.js compile",
    "watch": "node ./node_modules/gulp/bin/gulp.js watch",
    "compile-build": "node ./node_modules/gulp/bin/gulp.js compile-build",
    "compile-extensions-build": "node ./node_modules/gulp/bin/gulp.js compile-extensions-build",
    "minify": "node ./node_modules/gulp/bin/gulp.js minify",
    "hygiene": "node ./node_modules/gulp/bin/gulp.js hygiene",
    "eslint": "node ./node_modules/gulp/bin/gulp.js eslint",
    "monaco-compile-check": "cd build && npm ci && npm run compile && cd .. && node build/lib/preLaunch.js",
    "valid-layers-check": "node build/lib/layersChecker.js",
    "update-localization-extension": "npm run compile && node ./build/lib/updateLocalizationExtension.js",
    "smoketest": "node build/lib/preLaunch.js && cd test/smoke && npm ci && node test/index.js",
    "download-builtin-extensions": "node build/lib/builtInExtensions.js",
    "electron": "node build/lib/electron.js",
    "gulp": "node ./node_modules/gulp/bin/gulp.js",
    "test": "scripts/test.sh",
    "test-node": "cd test && ../node_modules/.bin/mocha",
    "test-browser-no-install": "cd test && node browser/index.js",
    "compile-web": "node ./node_modules/gulp/bin/gulp.js compile-web",
    "watch-web": "node ./node_modules/gulp/bin/gulp.js watch-web"
  },
  "devDependencies": {
    "ansi-colors": "^3.2.3",
    "byline": "^5.0.0",
    "debounce": "^1.0.0",
    "debug": "^4.4.1",
    "eslint": "^8.36.0",
    "event-stream": "^4.0.1",
    "extract-zip": "^2.0.1",
    "fancy-log": "^1.3.2",
    "file-type": "^10.9.0",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.4",
    "graceful-fs": "^4.2.2",
    "gulp": "^4.0.2",
    "gulp-azure-storage": "^0.10.0",
    "gulp-cli": "^2.3.0",
    "gulp-filter": "^5.1.0",
    "gulp-flatmap": "^1.0.2",
    "gulp-gzip": "^1.4.2",
    "gulp-json-editor": "^2.5.0",
    "gulp-merge-json": "^2.2.1",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-sort": "^2.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-util": "^3.0.8",
    "jsonc-parser": "^2.3.1",
    "mime": "^1.4.1",
    "minimist": "^1.2.0",
    "mocha": "^9.2.2",
    "npm-run-all": "^4.1.5",
    "pump": "^1.0.3",
    "rimraf": "^2.2.8",
    "source-map": "^0.7.3",
    "ternary-stream": "^3.0.0",
    "through": "^2.3.8",
    "through2": "^2.0.0",
    "typescript": "5.7.3",
    "vinyl": "^2.0.0",
    "vinyl-fs": "^3.0.0",
    "vinyl-sourcemaps-apply": "^0.2.1",
    "yauzl": "^2.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wildhash/vscode.git"
  },
  "bugs": {
    "url": "https://github.com/wildhash/vscode/issues"
  },
  "optionalDependencies": {
    "windows-foreground-love": "^0.4.0",
    "windows-mutex": "^0.4.2",
    "windows-process-tree": "^0.4.0"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.6.5"
  },
  "type": "module"
}
